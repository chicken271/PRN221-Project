<Window x:Class="PRN221_Project.AdminDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRN221_Project"
        mc:Ignorable="d"
        Title="AdminDashboard" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock="Top" Padding="20" Margin="10" Name="btnTeacher" Click="btnTeacher_Click">Teacher Management</Button>
            <Button DockPanel.Dock="Top" Padding="20" Margin="10" Name="btnStudent" Click="btnStudent_Click">Student Management</Button>
            <Button DockPanel.Dock="Top" Padding="20" Margin="10" Name="btnClass" Click="btnClass_Click">Class Management</Button>
            <Button Padding="20" Margin="10" Name="btnLogout" Click="btnLogout_Click">Logout</Button>
        </DockPanel>
    </Grid>
</Window>
